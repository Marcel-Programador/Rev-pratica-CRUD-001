<%- include("./partials/head")%>
<main>
    <h1>Products</h1>
    <% if(produtos.length > 0) { %>
        <% for(let produto of produtos) { %>
            <div>
                <h2><%= produto.nome %></h2>
                <span><%= produto.preco %></span>
                <p><%= produto.descricao %></p>
            </div>
        <% } %>
    <% }else { %>
        <h1>Nenhum produto encontrado</h1>
    <% } %>
</main>
<%- include("./partials/footer")%>